{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "itemId": {
      "type": "string"
    },
    "loanDate": {
      "type": "string",
      "pattern":"^\\d{4}-\\d{2}-\\d{2}$"
    },
    "returnDate": {
      "type": "string",
      "pattern":"^\\d{4}-\\d{2}-\\d{2}$"
    }
  },
  "required": [
    "userId",
    "itemId",
    "loanDate"
  ]
}
